{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "env": {
    "VERCEL_ENV": "production",
    "TEMP_DIR": "/tmp",
    "OUTPUT_DIR": "/tmp"
  }
}